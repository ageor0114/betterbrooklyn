{
  "name": "better-brooklyn",
  "version": "0.0.2",
  "description": "WIP boilerplate for Cooper Union Computer Science and Entrepreneurship for Social Good course",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start2": "concurrently \"npm:start:app\" \"npm:start:env\"",
    "start:app": "react-scripts start",
    "start:env": "docker-compose -f ./node_modules/@daostack/client/docker-compose.yml up graph-node",
    "stop:env": "docker-compose -f ./node_modules/@daostack/client/docker-compose.yml down -v"
  },
  "dependencies": {
    "@dorgtech/daocomponents": "0.0.5-alpha",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "bootstrap": "^4.3.1",
    "firebase": "^5.2.0",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.1",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.0",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
